<%- include("../partials/layout-start", {title: title}) %>
<%- include("../partials/menu") %>
<div class="row">
    <div class="col-sm-6">
        <h2><%= title %> <%= model.login %> (<%= model.name %>)</h2>
    </div>
</div>

<div class="form-group row">
    <label for="email" class="col-sm-2 col-form-label">Контактный Email:</label>
    <div class="col-sm-4">
        <input type="text" readonly class="form-control-plaintext" id="email" value="<%= model.email %>">
    </div>
</div>

<%- include("../partials/layout-end") %>
